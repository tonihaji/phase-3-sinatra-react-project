<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th>Status Pay</th>
      <th>Image</th>
    </tr>
  </thead>
  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= item.name %></td>
        <td><%= item.price %></td>
        <td><%= item.status_pay %></td>
        <td><%= image_tag item.image_url %></td>
      </tr>
    <% end %>
  </tbody>
</table>
