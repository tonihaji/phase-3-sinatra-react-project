
     <table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Model</th>
      <th>Rating</th>
      <th>Image</th>
    </tr>
  </thead>
  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= item.name %></td>
        <td><%= item.model %></td>
        <td><%= item.rating %></td>
        <td><%= image_tag item.image_url %></td>
      </tr>
    <% end %>
  </tbody>
</table> 